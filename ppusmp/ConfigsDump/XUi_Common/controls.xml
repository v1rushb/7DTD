      <rect name="directvalue" style="press, scroll, hover, drag" pos="${2+height},0" width="${width-height*2-4}" height="${height}" pivot="topleft" createuiwidget="true" tooltip_key="${tooltip_key}" gamepad_selectable="${gamepad_selectable}" />
